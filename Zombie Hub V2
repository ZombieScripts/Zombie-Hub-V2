local allowedUserId = 6158741138  -- Your UserId
local currentUserId = game.Players.LocalPlayer.UserId  -- Get the current player's UserId

local keyRequired = true  -- Default is that the key is required

if currentUserId == allowedUserId then
    keyRequired = false  -- If the player is allowed, they don't need the key
end

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

